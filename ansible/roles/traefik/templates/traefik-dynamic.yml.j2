http:
  routers:
    openclaw:
      rule: "Host(`{{ domain_name }}`)"
      entryPoints:
        - websecure
      service: openclaw
      tls:
        certResolver: letsencrypt
  services:
    openclaw:
      loadBalancer:
        servers:
          - url: "http://openclaw:18789"
        passHostHeader: true
